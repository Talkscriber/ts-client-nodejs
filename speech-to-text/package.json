{
  "name": "@talkscriber-npm/ts-client",
  "version": "1.0.15",
  "description": "A JS/TypeScript client module for Talkscriber",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./browser": {
      "import": "./dist/index.browser.js",
      "types": "./dist/index.browser.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "node build.js",
    "build:ui": "tsc --project examples/ui/tsconfig.json",
    "dev:ui": "npm run build:ui && http-server . -p 8320 -o /examples/ui/index.html",
    "prepublishOnly": "npm run build",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "example": "ts-node -r tsconfig-paths/register examples/nodejs/talkscriber_client.ts"
  },
  "keywords": [
    "client",
    "websocket",
    "audio",
    "talkscriber",
    "speech-to-text"
  ],
  "author": "Talkscriber <support@talkscriber.com>",
  "license": "MIT",
  "dependencies": {
    "@talkscriber-npm/ts-client": "^1.0.15",
    "wav-decoder": "^1.3.0",
    "ws": "^7.5.10"
  },
  "devDependencies": {
    "@types/node": "^20.19.21",
    "@types/ws": "^8.5.13",
    "http-server": "^14.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/talkscriber-npm/ts-client.git"
  },
  "bugs": {
    "url": "https://github.com/talkscriber-npm/ts-client/issues"
  },
  "homepage": "https://github.com/talkscriber-npm/ts-client#readme",
  "directories": {
    "example": "examples"
  }
}
